  "name": "react-app",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "generate": "plop --plopfile ./internals/generators/plopfile.js",
    "test:setup": "run-s \"start:theme -- {1}\" \"start:config staging {1}\" \"build-models \" \"test:run --CLIENT={1} --TEST_PATH={2}\" --",
    "test": "run-s \"test:run --CLIENT={1} --TEST_PATH={2}\" --",
    "test:run": "CLIENT=$npm_config_CLIENT jest --no-cache --watch ${npm_config_TEST_PATH:-src}",
    "test:staging": "CLIENT=$npm_config_CLIENT jest --no-cache src",
    "test:production": "CLIENT=$npm_config_CLIENT PRODUCTION=true jest --no-cache src",
    "test:ci": "bash jest/test-all-clients.bash",
    "build:analysis": "webpack --rtvanalysis --mode=production --TARGET_ENV=staging --progress --profile",
    "grunt:watch": "grunt watch",
    "production:app": "npm run build-models && webpack --mode=production --TARGET_ENV=production",
    "production": "run-s \"start:theme -- {1}\" \"start:config production {1} \" \"production:app -- {2} --CLIENT={1}\" --",
    "staging:app": "npm run build-models && webpack --mode=production --TARGET_ENV=staging",
    "staging": "run-s \"start:theme -- {1}\" \"start:config staging {1}\" \"staging:app -- {2}  --CLIENT={1}\" --",
    "start:theme": "node copy-theme",
    "start:config": "node ./scripts/generateConfiguration.js all",
    "start:app": "webpack-dev-server --mode=development --TARGET_ENV=development --progress",
    "start:app:ie": "webpack-dev-server --mode=development --TARGET_ENV=development --progress --iedevmode=true",
    "start": "npm-run-all \"start:theme -- {1}\" \"start:config staging {1} \" -p build-models \"grunt:watch --{2} --CLIENT={1} \" \"start:app -- {2} --CLIENT={1}\" --",
    "start:ie": "npm-run-all \"start:theme -- {1}\" \"start:config staging {1} \" -p build-models \"grunt:watch --{2} --CLIENT={1} \" \"start:app:ie -- {2} --CLIENT={1}\" --",
    "start:app:ci": "webpack-dev-server --mode=development --TARGET_ENV=development --port 8080 --progress",
    "start:ci": "npm-run-all \"start:theme -- {1}\"  -p \"grunt:watch --{2} --CLIENT={1} \" \"start:app:ci -- {2} --CLIENT={1}\" --",
    "storybook": "npm-run-all \"start:theme -- {1}\"  -p \"grunt:watch --{2} --CLIENT={1} \" \"start:storybook -- {1}\" --",
    "start:storybook": "bash ./.storybook/start.sh",
    "build-storybook": "build-storybook",
    "watch": "tsc --watch --allowJs false",
    "typecheck:ci": "node copy-theme rtv && tsc --allowJs false",
    "postinstall": "rm -rf node_modules/@types/react-native && rm -rf node_modules/@storybook/react/dist/client/preview/index.d.ts",
    "lite": "npm-run-all \"start:theme -- rtv\" \"start:config\"  -p build-models \"grunt:watch --{2} --CLIENT=rtv \" \"start:app --{2} --CLIENT=rtv --LITE_CLIENT={1}\" --",
    "build-models": "ts-node src/javascript/models/createModelJson.ts --transpile-only",
    "lint": "eslint src --quiet",
    "securityChecks": "bash ./scripts/securityChecks.sh",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config defaultCommandTimeout=20000",
    "docker:cypress:open": "docker-compose -f docker-compose-test.yml up",
    "docker:cypress:run": "docker-compose -f docker-compose-test.yml run test_app npm run test:docker:run",
    "test:docker:open": "./node_modules/.bin/start-server-and-test \"npm run start:ci $CLIENT\" http://localhost:8080 \"npm run cypress:open --  --config baseUrl=http://localhost:8080 --env CLIENT=$CLIENT\"",
    "test:docker:run": "./node_modules/.bin/start-server-and-test \"npm run start:ci $CLIENT\" http://localhost:8080 \"npm run cypress:run --  --config baseUrl=http://localhost:8080 --env CLIENT=$CLIENT\"",
    "test:integration": "start-server-and-test \"npm run start:ci $CLIENT\" http://localhost:8080 \"npm run cypress:run -- --env CLIENT=$CLIENT\"",
    "test:integration:headed": "start-server-and-test \"npm run start:ci $CLIENT\" http://localhost:8080 \"npm run cypress:run -- --headed --env CLIENT=$CLIENT\""
  },
